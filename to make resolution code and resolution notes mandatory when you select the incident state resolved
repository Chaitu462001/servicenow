function onChange(control, oldValue, newValue, isLoading, isTemplate){
    }
    if (newValue === '') {
        g_form.setMandatory('field_name', false);
        g_form.setMandatory('field_name', false);
        return;
    }

    if (newValue === 'resolved') {
        g_form.setMandatory('field_name', true);
        g_form.setMandatory('field_name', true);
    } else {
        g_form.setMandatory('field_name', false);
        g_form.setMandatory('field_name', false);
    }
